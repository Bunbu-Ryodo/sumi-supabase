{
  "expo": {
    "name": "sumi-supabase",
    "slug": "sumi-supabase",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/sumi-android-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/sumi-android-icon.png",
        "backgroundColor": "#F6F7EB"
      },
      "package": "com.anonymous.sumisupabase"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/sumi-splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#393E41"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/sumi-splash-icon.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": false
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
